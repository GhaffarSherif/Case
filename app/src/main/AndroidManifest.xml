<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cstuser.casestudy" >
    <uses-sdk android:minSdkVersion="11" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
        android:name=".MainActivity"
        android:label="@string/app_name" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
        <activity
            android:name=".MainMenu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.ctuser.casestudy.MainMenu" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".slideshow.SlideshowActivity"
            android:label="@string/title_activity_slideshow" >
            <intent-filter>
                <action android:name="com.example.cstuser.casestudy.SlideshowActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Database.PlayerDatabaseMenu"
            android:label="@string/title_activity_slideshow" >
            <intent-filter>
                <action android:name="com.example.cstuser.casestudy.Database.PlayerDatabaseMenu" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
        android:name=".Database.TeamDatabaseMenu"
        android:label="Swag ">
        <intent-filter>
            <action android:name="com.example.cstuser.casestudy.Database.TeamDatabaseMenu" />
            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
        </activity>
        <activity
            android:name=".Database.AddEditDeleteRecord"
            android:label="Add, edit or delete record">
            <intent-filter>
                <action android:name="com.example.cstuser.casestudy.Database.AddEditDeleteRecord" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AnimationCase"
            android:label="Add, edit or delete record">
            <intent-filter>
                <action android:name="com.example.cstuser.casestudy.AnimationCase" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

    </application>

</manifest>
